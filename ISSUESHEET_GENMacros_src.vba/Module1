Sub Macro2()
'
' Macro2 Macro
'

'
    ActiveSheet.Shapes.Range(Array("LoaDOHing")).Select
    Selection.ShapeRange.ScaleWidth 0.0001, msoFalse, msoScaleFromTopLeft
    Selection.ShapeRange.ScaleHeight 0.0001, msoFalse, msoScaleFromTopLeft
    Range("M8").Select
    ActiveSheet.Shapes.Range(Array("LoaDOHing")).Select
    Selection.ShapeRange.ScaleWidth 13.1252931539, msoFalse, msoScaleFromTopLeft
    Selection.ShapeRange.ScaleHeight 13.1252870798, msoFalse, msoScaleFromTopLeft
    Range("M8").Select
End Sub